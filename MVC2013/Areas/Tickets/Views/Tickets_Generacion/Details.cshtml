@model MVC2013.Models.Tickets_Generacion

@{
    ViewBag.Title = "Details";
    Layout = "~/Areas/Tickets/Views/Shared/_Layout.cshtml";
}
<div class="form-group">
    <button class="btn-bitbucket btn-sm" onclick="location.href = '@Url.Action("Index")'">
        <span class="glyphicon glyphicon-backward"></span>&nbsp;@Resources.Resources.listado_regresar
    </button>
</div>
<h2>Detalles de la Generacion</h2>
<div>
    <dl class="dl-horizontal">
        <dt>Tipo de Ticket</dt>
        <dd>@Html.DisplayFor(model => model.Tickets_Tipo.nombre)</dd>
        <dt>Cantidad</dt>
        <dd>@Html.DisplayFor(model => model.cantidad)</dd>
        <dt>Desde</dt>
        <dd>@Html.DisplayFor(model => model.desde)</dd>
        <dt>Hasta</dt>
        <dd>@Html.DisplayFor(model => model.hasta)</dd>
    </dl>
    <dl class="dl-horizontal">
        <dd>
            <button class="btn btn-primary btn-sm" onclick="location.href = '@Url.Action("Edit", new { id = Model.id_ticket_generacion })'">
                <span class="glyphicon glyphicon-edit"></span>&nbsp;Editar
            </button>
        </dd>
    </dl>
</div>